class Hp{
	public static void convertFive(int n){
    int sum=0;
    int temp=n;
    int place=0;
    while(temp>0)
    {   
        int rem=temp%10;
        temp=temp/10;
        if(rem==0)
        {
            sum+=5*Math.pow(10,place);
        }
        place+=1;
    }
    System.out.println(n+sum);
    
    //OR
        /*String s=String.valueOf(n);
	s=s.replace('0','5');
	int x=Integer.parseInt(s);
	System.out.println(x);*/
}
//Main Function
}
